# Backend server configuration
BACKEND_LISTEN_HOST="" # Host address for backend server (e.g., "localhost" or "0.0.0.0")
BACKEND_LISTEN_PORT="" # Port number for backend server (e.g., "3001")
BACKEND_JWT_SECRET_KEY="" # Secret key for JWT token signing and verification

# Deployment server credentials
DEPLOY_SERVER_HOST="" # Remote server hostname/IP for deployment
DEPLOY_SERVER_PASSWORD="" # SSH password for deployment server
DEPLOY_SERVER_USERNAME="" # SSH username for deployment server

# MySQL database configuration
DB_DATABASE_NAME="" # Name of the MySQL database
DB_PORT="" # MySQL server port (typically "3306")
DB_ROOT_PASSWORD="" # Root user password for MySQL
DB_PROPERTY_USER_PASSWORD="" # Password for property-specific database user
DATABASE_URL="" # Full Prisma database connection string (mysql://user:pass@host:port/db)

# Redis cache configuration
REDIS_PORT="" # Redis server port (typically "6379")
REDIS_URL="" # Full Redis connection string (redis://host:port)

# External API keys
NSW_TRANSPORT_API_KEY="" # NSW Transport API key for property scraping service
DEEPSEEK_API_KEY="" # DeepSeek AI API key for backend AI features
SENDGRID_API_KEY="" # SendGrid API key for email service

# Scraper service API keys
GOOGLE_MAPS_API_KEY="" # Google Maps API key for commute time calculation
PROPERTY_RATING_API_KEY="" # DashScope API key for property rating and keyword extraction

# Scraper database configuration
DB_HOST="" # Database host (use service name "db" in Docker Compose)
DB_USER="" # Database username for scraper (typically "property_user")
DB_PASSWORD="" # Database password for scraper (same as DB_PROPERTY_USER_PASSWORD)
DB_DATABASE="" # Database name (same as DB_DATABASE_NAME)

# Frontend configuration
NEXT_PUBLIC_BACKEND_URL="" # Backend URL accessible from frontend (e.g., "http://localhost:3001")
NOTION_DATABASE_ID="" # Notion database ID for frontend content
NOTION_TOKEN="" # Notion integration token for frontend content access
GEMINI_API_KEY="" # API key for frontend AI features

# Agent configuration
AGENT_HOST="" # Host address where the Agent service is running
AGENT_PORT="" # Port number exposed by the Agent service
OPENAI_API_KEY="" # API key for OpenAI models
BAILIAN_API_KEY="" # API key for Alibaba Bailian / DashScope services
LANGSMITH_TRACING="" # Enable LangSmith tracing for agent execution
LANGSMITH_API_KEY="" # API key for LangSmith